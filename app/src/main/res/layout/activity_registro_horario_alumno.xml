<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".registroHorarioAlumno"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="12dp"
        android:text="Grupo:"
        app:layout_constraintBottom_toBottomOf="@+id/spinnerGrupo"
        app:layout_constraintEnd_toStartOf="@+id/spinnerGrupo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Spinner
        android:id="@+id/spinnerGrupo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerMateria"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="62dp"
        android:text="Materia:"
        app:layout_constraintBottom_toTopOf="@+id/spinnerMateria"
        app:layout_constraintEnd_toStartOf="@+id/spinnerMateria"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spinnerMateria" />

    <Spinner
        android:id="@+id/spinnerMateria"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerEdificio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/spinnerGrupo" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginBottom="19dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="20dp"
        android:text="Salón:"
        app:layout_constraintBottom_toTopOf="@+id/spinnerLunes"
        app:layout_constraintEnd_toStartOf="@+id/spinnerEdificio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spinnerMateria" />

    <Spinner
        android:id="@+id/spinnerEdificio"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerLunes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/spinnerMateria" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="62dp"
        android:text="Lunes:"
        app:layout_constraintBottom_toTopOf="@+id/spinnerLunes"
        app:layout_constraintEnd_toStartOf="@+id/spinnerLunes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spinnerLunes" />

    <Spinner
        android:id="@+id/spinnerLunes"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="29dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerMartes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/spinnerEdificio" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginBottom="31dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="31dp"
        android:text="Martes:"
        app:layout_constraintBottom_toBottomOf="@+id/spinnerMiercoles"
        app:layout_constraintEnd_toStartOf="@+id/spinnerMartes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerLunes" />

    <Spinner
        android:id="@+id/spinnerMartes"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerMiercoles"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/spinnerLunes" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="24dp"
        android:text="Miércoles:"
        app:layout_constraintBottom_toBottomOf="@+id/spinnerJueves"
        app:layout_constraintEnd_toStartOf="@+id/spinnerMiercoles"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerMartes" />

    <Spinner
        android:id="@+id/spinnerMiercoles"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerJueves"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/spinnerMartes" />


    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginBottom="34dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="34dp"
        android:text="Jueves:"
        app:layout_constraintBottom_toBottomOf="@+id/spinnerViernes"
        app:layout_constraintEnd_toStartOf="@+id/spinnerJueves"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <Spinner
        android:id="@+id/spinnerJueves"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="23dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerViernes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/spinnerMiercoles" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginEnd="33dp"
        android:layout_marginStart="62dp"
        android:text="Viernes:"
        app:layout_constraintBottom_toBottomOf="@+id/spinnerViernes"
        app:layout_constraintEnd_toStartOf="@+id/spinnerViernes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerViernes" />

    <Spinner
        android:id="@+id/spinnerViernes"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="38dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonRegistrarHorarioAlumno"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/spinnerJueves" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="145dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="95dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="16dp"
        android:text="Registro Horario"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="@+id/txtBoleta"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="35dp"
        android:layout_marginEnd="70dp"
        android:layout_marginStart="62dp"
        android:text="Boleta:"
        app:layout_constraintBottom_toTopOf="@+id/spinnerGrupo"
        app:layout_constraintEnd_toStartOf="@+id/txtBoleta"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/txtBoleta"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="36dp"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttonRegistrarHorarioAlumno"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="62dp"
        android:text="Registrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerViernes" />

</android.support.constraint.ConstraintLayout>